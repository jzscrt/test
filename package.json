{
  "name": "cybersweep-server-ts",
  "version": "1.0.0",
  "description": "CyberSweep API Server: TypeScript + NodeJS + Express",
  "main": "src/server.ts",
  "scripts": {
    "start": "npm run build && cross-env NODE_ENV=production node dist/server.js",
    "dev": "cross-env NODE_ENV=development nodemon",
    "build": "npx swc src -d dist --source-maps --copy-files",
    "build:tsc": "npx tsc && npx tsc-alias",
    "lint": "eslint --ignore-path .gitignore --ext .js,.ts,.tsx .",
    "lint:fix": "npm run lint -- --fix",
    "test": "cross-env NODE_ENV=development jest --watch --config jest.config.ts",
    "cdk:synth": "npx aws-cdk synth --app 'npx ts-node bin/cdk.ts'",
    "cdk:deploy": "npx aws-cdk deploy --app 'npx ts-node bin/cdk.ts'",
    "cdk:cleanup": "npx aws-cdk destroy --app 'npx ts-node bin/cdk.ts'",
    "cdk:bootstrap": "npx cdk bootstrap --app 'npx ts-node bin/cdk.ts'"
  },
  "author": "Jim Zandueta <jimz@regalcredit.com>",
  "license": "ISC",
  "dependencies": {
    "@meanie/mongoose-to-json": "^2.6.0",
    "aws-cdk": "^2.61.0",
    "aws-cdk-lib": "^2.60.0",
    "aws-lambda": "^1.0.7",
    "bcrypt": "^5.1.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "cors": "^2.8.5",
    "dayjs": "^1.11.7",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "helmet": "^6.0.1",
    "http-status": "^1.6.2",
    "jsonwebtoken": "^9.0.0",
    "mongoose": "^6.8.4",
    "passport": "^0.6.0",
    "passport-jwt": "^4.0.1"
  },
  "devDependencies": {
    "@swc/cli": "^0.1.57",
    "@swc/core": "^1.3.27",
    "@types/aws-lambda": "^8.10.109",
    "@types/bcrypt": "^5.0.0",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.15",
    "@types/jest": "^29.2.5",
    "@types/node": "^18.11.18",
    "@types/passport": "^1.0.11",
    "@types/passport-jwt": "^3.0.8",
    "@types/supertest": "^2.0.12",
    "@types/swagger-jsdoc": "^6.0.1",
    "@types/swagger-ui-express": "^4.1.3",
    "@typescript-eslint/eslint-plugin": "^5.48.2",
    "@typescript-eslint/parser": "^5.48.2",
    "assert": "^2.0.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.32.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-prettier": "^4.2.1",
    "jest": "^29.3.1",
    "joi": "^17.7.0",
    "nodemon": "^2.0.20",
    "supertest": "^6.3.3",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^4.6.0",
    "ts-jest": "^29.0.5",
    "ts-node": "^10.9.1",
    "tsc-alias": "^1.7.0",
    "tsconfig-paths": "^4.1.0",
    "tsconfig-paths-jest": "^0.0.1",
    "tslib": "^2.4.0",
    "typescript": "^4.8.3",
    "winston": "^3.8.2",
    "winston-daily-rotate-file": "^4.7.1"
  }
}
